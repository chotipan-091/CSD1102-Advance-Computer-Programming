import java.util.Scanner;

public class GuessNumberGame {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        // ‡∏™‡∏∏‡πà‡∏°‡πÄ‡∏•‡∏Ç‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á 1 - 20 ‡πÇ‡∏î‡∏¢‡πÑ‡∏°‡πà‡πÉ‡∏ä‡πâ Random library
        int secretNumber = (int)(Math.random() * 20) + 1;
        int maxTries = 5;

        System.out.println("=== ‡πÄ‡∏Å‡∏°‡∏ó‡∏≤‡∏¢‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç (1-20) ===");
        System.out.println("‡∏Ñ‡∏∏‡∏ì‡∏°‡∏µ‡∏™‡∏¥‡∏ó‡∏ò‡∏¥‡πå‡∏ó‡∏≤‡∏¢‡πÑ‡∏î‡πâ 5 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á");

        for (int i = 1; i <= maxTries; i++) {
            System.out.print("‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà " + i + " ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏ó‡∏≤‡∏¢‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç: ");
            int guess = sc.nextInt();

            if (guess == secretNumber) {
                System.out.println("üéâ ‡∏¢‡∏¥‡∏ô‡∏î‡∏µ‡∏î‡πâ‡∏ß‡∏¢! ‡∏Ñ‡∏∏‡∏ì‡∏ó‡∏≤‡∏¢‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á");
                break;
            } else if (guess < secretNumber) {
                System.out.println("‚ùå ‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ");
            } else {
                System.out.println("‚ùå ‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡∏°‡∏≤‡∏Å‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ");
            }

            if (i == maxTries) {
                System.out.println("‡πÄ‡∏Å‡∏°‡∏à‡∏ö‡πÅ‡∏•‡πâ‡∏ß! ‡πÄ‡∏•‡∏Ç‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡∏Ñ‡∏∑‡∏≠ " + secretNumber);
            }
        }

        sc.close();
    }
}